				"typeProperties": {
					"sheetName": "Asset",
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "Master.xlsx",
						"folderPath": "GoldRush_MF_Finance_db",
						"container": "gr-demo-container"
					},
					"firstRowAsHeader": true
